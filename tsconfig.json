{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "contentlayer/generated": [
        "./.contentlayer/generated"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true,
    "types": [
      "next",
      "next/image-types/global"
    ],
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".contentlayer/generated",
    "lib/firebase.js",
    "app/dashboard-two/page.jsx",
    "app/productivity-tools/data/page.tsx",
    "lib/auth.txtx",
    "app/productivity-tools/layoutt.tsx",
    "app/productivity-tools/page.jsx",
    "app/mark/layoutjsx",
    "components/Google.js",
    "app/dashboard/page.tsx",
    "components/icons.tsx",
    "components/icons/IconGrid.tsx",
    "lib/useMousePosition.tsx",
    "components/header/custom-menu.tsx",
    "app/thoughts/layout.tsx",
    "app/html-to-jsx/playground/AppContext.tsx",
    "components/icons/TrashIcon.js",
    "components/core/Cursor/GreenTrailer",
    "app/thoughts/components/ThoughtCard.tsx"
,
"app/(regular)/(nolayout)/geolocation/components/google-long-lat-map.tsx"
],
  "exclude": [
    "node_modules"
  ],
}